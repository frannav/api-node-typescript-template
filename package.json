{
	"name": "node-js-api",
	"version": "1.0.0",
	"description": "ATM API",
	"main": "dist/index.js",
	"type": "module",
	"engines": {
		"node": ">=18.0.0"
	},
	"scripts": {
		"start": "node --env-file-if-exists=.env dist/index.js",
		"build": "tsc",
		"dev": "tsx --env-file-if-exists=.env --watch src/index.ts",
		"format": "npx biome format --write .",
		"lint": "npx biome lint --write .",
		"lint:check": "npx biome check .",
		"lint:check:fix": "npx biome check . --write",
		"ci:check": "npx biome ci .",
		"test": "npm run test:unit && npm run test:integration",
		"test:unit": "tsx --env-file-if-exists=.env --test src/modules/**/*.test.ts",
		"test:integration": "tsx --env-file-if-exists=.env --test test/integration/**/*.test.ts"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"@biomejs/biome": "2.1.3",
		"@types/express": "^4.17.21",
		"@types/node": "^24.3.0",
		"ts-node": "^10.9.2",
		"tsx": "^4.20.4",
		"typescript": "^5.9.2"
	},
	"dependencies": {
		"@types/bcrypt": "^6.0.0",
		"@types/cors": "^2.8.19",
		"@types/swagger-jsdoc": "^6.0.4",
		"@types/swagger-ui-express": "^4.1.8",
		"bcrypt": "^6.0.0",
		"cors": "^2.8.5",
		"express": "^4.19.2",
		"pino": "^9.7.0",
		"pino-http": "^10.5.0",
		"pino-pretty": "^13.1.1",
		"swagger-jsdoc": "^6.2.8",
		"swagger-ui-express": "^5.0.1",
		"zod": "^3.23.8"
	}
}